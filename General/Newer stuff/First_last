'''
a = [5, 10, 15, 20, 25]

first_last = []

new_list =[a[0],a[-1]]
print(new_list)
'''

import random

def get_random_list():
    random_list = list(random.sample(range(75),random.randint(0,25)))
    print(f'The list is {random_list}')
    return random_list

def first_last(random_list):
    new_list = [random_list[0],random_list[-1]]
    return new_list

whatever = first_last(get_random_list())
print(whatever)